@model IEnumerable<Library.Domain.ArkStory_EditList>

@{
    ViewData["Title"] = "View";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>View</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.IsChild)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.IsDirty)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.IsValid)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.IsSavable)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.AllowEdit)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.AllowNew)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.AllowRemove)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.RaiseListChangedEvents)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Count)
                </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.IsChild)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IsDirty)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IsValid)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IsSavable)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AllowEdit)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AllowNew)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AllowRemove)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RaiseListChangedEvents)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Count)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { /* id=item.PrimaryKey */ }) |
                @Html.ActionLink("Details", "Details", new { /* id=item.PrimaryKey */ }) |
                @Html.ActionLink("Delete", "Delete", new { /* id=item.PrimaryKey */ })
            </td>
        </tr>
}
    </tbody>
</table>
